{
  "expo": {
    "name": "React Native Sandbox",
    "slug": "react-native-sandbox",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#9AC2C9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.yourcompany.yourappname",
      "adaptiveIcon": {
        "backgroundColor": "#9AC2C9",
        "foregroundImage": "./src/assets/icon-android.png"
      }
    }
  }
}
