{
  "expo": {
    "name": "Smovie",
    "description": "Find movies & tv shows guide recommandations - Looking for a movie or series idea for tonight? Smovie is the simplest and fastest way to discover movies, series and actors. It's an application based on themoviedb (TMDb)database.",
    "slug": "smovie",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "backgroundColor": "#0f1115",
    "primaryColor": "#5772B9",
    "githubUrl": "https://github.com/theo-mesnil/smovie",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#5772B9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.theomesnil.smovie",
      "versionCode": 4,
      "adaptiveIcon": {
        "backgroundColor": "#5772B9",
        "foregroundImage": "./src/assets/icon-android.png"
      }
    }
  }
}
